<h1>Edit that event boi</h1>

<%= form_for @event do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :time %>
  <%= f.text_field :time %>
  <br>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <br>
  <%= f.label :description %>
  <%= f.text_area :description %>
  <br>
  <%= f.label :longitude %>
  <%= f.text_area :longitude %>
  <br>
  <%= f.label :latitude %>
  <%= f.text_area :latitude %>
  <br>
  <%= f.submit %>
<% end %>