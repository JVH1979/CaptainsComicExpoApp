<h1>Edit Event</h1>

<%= form_for @event do |f| %>
  <%= f.label :event_name %>
  <%= f.text_field :event_name %>
  <br>
  <%= f.label :event_time %>
  <%= f.text_field :event_time %>
  <br>
  <%= f.label :event_location %>
  <%= f.text_field :event_location %>
  <br>
  <%= f.label :event_description%>
  <%= f.text_area :event_description %>
  <br>
  <%= f.submit %>
<% end %>